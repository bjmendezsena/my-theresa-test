{
  "name": "my-theresa-test",
  "version": "1.0.0",
  "description": "A test project for MyTheresa using express and Vite",
  "type": "module",
  "homepage": "https://bjmendezsena.github.io/my-theresa-test",
  "scripts": {
    "dev": "tsx server.ts",
    "start": "NODE_ENV=production node dist/server.js",
    "dev:server": "tsx server.ts",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "test": "vitest run",
    "test:u": "vitest run --update",
    "test:w": "vitest watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "serve": "npm run build && cross-env NODE_ENV=production node ./dist/server.js",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles \"public/**/*\" dist && copyfiles -u 2 \"dist/client/**/*\" dist && copyfiles -u 2 \"dist/client/assets/**/*\" dist/public",
    "format": "prettier --write src types"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "@types/serve-static": "^1.15.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.526.0",
    "postcss-nesting": "^13.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.7.0",
    "serve-static": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/eslint": "^8.56.12",
    "@types/express": "^4.17.23",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.89.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  }
}
